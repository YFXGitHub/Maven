<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.yfx.springbootmybatis2.dao.RecordMapper">

    <select id="findById" resultType="com.yfx.springbootmybatis2.entity.Record">
        select * from record where id = #{id}
    </select>
</mapper>